<div class="alignleft">
  <% if (config.author){ %>
  &copy; <%= new Date().getFullYear() %> <%= config.author %>
  <% } else { %>
  &copy; <%= new Date().getFullYear() %> <%= config.title %>
  <% } %>
  <% if (theme.society.enable){ %>
    <a class="icon github" href="<%= theme.society.github %>">github</a>
    <a href="<%= theme.society.weibo %>">weibo</a>
    <a href="<%= theme.society.google_plug %>">google+</a>
  <% } %>
</div>

<div class="clearfix"></div>